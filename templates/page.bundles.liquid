          <script>
           function addItem(id, qty, properties, callback) {
              var params = {
                quantity: qty,
                id: id
              };
              if(properties != false){
                params.properties = properties;
              }
              $.ajax({
                type: 'POST',
                url: '/cart/add.js',
                dataType: 'json',
                data: params,
                success: function(){ 
                $('#bundle-page .modal:visible').hide(); 
