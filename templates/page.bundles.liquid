        <script>
            $('#click-to-start').click(function() {
              $('#step-one-who').show()
              $('html, body').animate({
                  scrollTop: $("#step-one-who").offset().top - 166.53
                }, 500);
            })
          

            $('#bundle-scroll').click(function() {
              $('html, body').animate({
                scrollTop: $("#bundle-refresh-wrapper").offset().top -  148.3
              }, 500);
            })
            $('#womens-bundle-choice, #mens-bundle-choice').click(function() {
              $('.icon').removeClass('highlight')
              $(this).find('.icon').addClass('highlight'); 
              $('#step-two-what').show()
              $('#bundle-refresh-wrapper').show()
            })
            $('.modal-not-featured').click(function() {
              newSrc = $(this).attr("src")

              $(this).parent().parent().siblings('.modal-featured').attr("src", newSrc).show().siblings('img').hide(); 
            })
            $('.bundle-modal-close').click(function() {
              $(this).parent().parent().hide(); 
              $('header, main, footer, #banner-cta, .index-sections, .mobile-nav1').removeClass('darken');
            })

            $('.bundle-color-selection').click(function() {
              $(this).addClass('highlight').siblings('.bundle-color-selection').removeClass('highlight'); 
              if ($(this).hasClass('black')) {
                $(this).parent().siblings('.black-color-choice').show().siblings('img').hide(); 
              }
              else if ($(this).hasClass('blue')) {
                $(this).parent().siblings('.blue-color-choice').show().siblings('img').hide(); 
              }
              else if ($(this).hasClass('rose')) {
                $(this).parent().siblings('.rose-color-choice').show().siblings('img').hide(); 
              }
              else if ($(this).hasClass('green')) {
                $(this).parent().siblings('.green-color-choice').show().siblings('img').hide(); 
              }
              else if ($(this).hasClass('beige')) {
                $(this).parent().siblings('.beige-color-choice').show().siblings('img').hide(); 
              }
            }); 

        		$('.bundle-style').click(function() {
        			var sku =  "." + $(this).attr("id")
        			$(sku).show(); 
      				$('header, main, footer, #banner-cta, .index-sections, .mobile-nav1').addClass('darken');
        		})
        		$('#womens-bundle-choice, #mens-bundle-choice').click(function() {
        			if ($(this).attr("id") == "womens-bundle-choice") {
        				$('.womens-bundle').show()
        				$('.mens-bundle').hide(); 
        			}
        			else if ($(this).attr("id") == "mens-bundle-choice") {
        				$('.womens-bundle').hide()
        				$('.mens-bundle').show(); 
        			}
               $('html, body').animate({
                  scrollTop: $("#step-two-what").offset().top -  148.3
                }, 500);
        		})
