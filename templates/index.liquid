  <script>
      $(document).ready(function() {

       setInterval(function(){ 
        var visibleSlide = $('.slideshow > div:visible').attr("id")
        if (visibleSlide == "slide5") {
          $('.slideshow > div:visible').hide();
          $('#slide1').show(); 
          $('.circle.background-green').removeClass('background-green').addClass('background-darkgray')
          $('.circle:first-of-type').addClass('background-green').removeClass('background-darkgray')
        }
        else {          
          $('.slideshow > div:visible').hide().next('.slideshow > div').show()
          $('.circle.background-green').removeClass('background-green').addClass('background-darkgray').next('.circle').addClass('background-green').removeClass('background-darkgray')
        }
        }, 5000);
      })

      $('.circle').click(function() {
        var newSlide = "#" + $(this).data("slide")
         $('.slideshow > div:visible').hide()
         $(newSlide).show()
         $(this).addClass('background-green').removeClass('background-darkgray').siblings().removeClass('background-green').addClass('background-darkgray')
      })
       </script>
