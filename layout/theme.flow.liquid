  <body class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '/', '-' | split: '.' }} {% if template == 'index' or page.handle == 'store' %}zoom-aware{% endif %}">
    <header>
      <div class="wrap cbb-ignored-image" style="text-align: center;">
        <a href="/?sh=d">
            <img data-src="{{'jlogo.png' | asset_url}}" alt="Gekks" class="logo lazyload" />
        </a>
        <div class="desktop" id="top-nav">
          <nav>
            <ul class="desktop-nav">
              {% for link in linklists.main-menu-uu.links %}
                {% assign child_list_handle = link.title | handleize %}
                {% if linklists[child_list_handle].links != blank %}
                  <ul class="childlink desktop-sub-nav">
                    {% for childlink in linklists[child_list_handle].links limit: 1 %}
                      <li class="sub-nav-header"><a href="{{ childlink.url }}">{{ childlink.title | escape }}</a></li>
                    {% endfor %}
                    <div class="sub-nav">
                      {% for childlink in linklists[child_list_handle].links offset: 1 %}
                     {% unless childlink.title == "Create Your Pack" %}
                      <li><a href="{{ childlink.url }}">{{ childlink.title | escape }}</a></li>
                    {% endunless %}
                      {% endfor %}
                    </div>
                  </ul>
               {% else %}
                <li class="no-sub"><a href="{{ link.url }}">{{ link.title | escape }}</a></li>
              {% endif %}
             {% endfor %}  
            </ul>
            
          
    
            
         
          </nav>
            <div class="customer-portal">
              {% if customer %}
                <a href="/account/"><img src="{{'login.png' | asset_url}}" alt="View Account"/></a>
              {% else %}
                <a href="/account/login"><img src="{{'login.png' | asset_url}}" alt="Login/Register"/></a>
              {% endif %}
            </div>
              <a href="/cart/" class="cart"><span>{{ cart.item_count }}</span> <image class="cart-icon" src="{{'shoppingbag.svg' | asset_url}}" style="margin-bottom: -11px;"/></a>
         </div>
      </div>
    </header>

  {{ content_for_layout }}
    
        
    <footer class="background-green">
        <div class="social-icons">
          <a href="https://www.instagram.com/gekks_go_sockless/?hl=en" rel="noreferrer" target="_blank"><img data-src="{{'ig.png' | asset_url}}" class="lazyload" alt="Instagram" /></a>
          <a href="https://www.facebook.com/GekksLowCutLiners/" target="_blank" rel="noreferrer"><img data-src="{{'fb.png' | asset_url}}" alt="Facebook" class="lazyload" /></a>
          <a href="https://www.pinterest.com/gekksliners/" target="_blank" rel="noreferrer"><img data-src="{{'pinterest.png' | asset_url}}" alt="Pinterest" class="lazyload" /></a>
      </div>
        <div id="footer-nav">
          <div id="nav-wrap">
                 <ul>
                <h3>company</h3>
                {% for link in linklists.footer-1.links limit: 5 offset: 5%}<li><a href="{{ link.url }}">{{ link.title | escape }}</a></li>{% endfor %}
              </ul>
              <ul>
                <h3>help</h3>
                {% for link in linklists.footer-1.links limit: 5%}<li><a href="{{ link.url }}">{{ link.title | escape }}</a></li>{% endfor %}
              </ul>
                <ul>
                <h3>shop</h3>
                {% for link in linklists.footer-1.links limit: 3 offset: 10%}
                  {% if link.title == "Men" or link.title == "Women" %}
                    <li><a href="/collections/women">{{ link.title | escape }}</a></li>
                  {% else %}
                    <li><a href="{{ link.url }}">{{ link.title | escape }}</a></li>
                  {% endif %}
                {% endfor %}
              </ul>
            </div>
            <div id="footer-aside">








          </div>
          <div class="background-darkgray">
            <div class="fw flex-container-no" id="payment-options">
              <img data-src="{{'amazon-pay.png' | asset_url}}" class="payment-icon lazyload" alt="amazon pay"/>
              <img data-src="{{'amex.png' | asset_url}}" class="payment-icon lazyload" alt="amex"/>
              <img data-src="{{'apple-pay.png' | asset_url}}" class="payment-icon lazyload" alt="apple pay"/>
              <img data-src="{{'discover.png' | asset_url}}" class="payment-icon lazyload" alt="discover"/>
              <img data-src="{{'google-pay.png' | asset_url}}" class="payment-icon lazyload" alt="google pay"/>
              <img data-src="{{'mastercard.png' | asset_url}}" class="payment-icon lazyload" alt="mastercard" />
              <img data-src="{{'paypal.png' | asset_url}}" class="payment-icon lazyload" alt="paypal" />
              <img data-src="{{'visa.png' | asset_url}}" class="payment-icon lazyload" alt="visa"/>
            </div>
            <h3 class="copywrite tac text-white nmb">Â© {{ shop.name }} {{ 'now' | date: '%Y' }}
            <p class="subscript nm tac">US Patent 9,839,259B2</p></h3>
          </div>
    </footer>

    
